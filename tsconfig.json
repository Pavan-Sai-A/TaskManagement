{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "paths": {
        "@type/*": [
          "./src/types/*"
        ],
        "@utils/*": [
          "./src/utils/*"
        ],
        "@assets/*": [
          "./src/assets/*"
        ],
        "@config/*": [
          "./src/config/*"
        ],
        "@pages/*": [
          "./src/pages/*"
        ],
        "@components/*": [
          "./src/components/*"
        ],
        "@store/*": [
          "./src/store/*"
        ],
        "@Hooks/*": [
          "./src/Hooks/*"
        ]
      },
    },
    "include": [
      "src",
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      ".next/types/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ],
  }
  